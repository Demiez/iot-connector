{
  "name": "iot-connector",
  "version": "1.0.0",
  "description": "IoT Connector",
  "main": "./dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Demiez/iot-connector"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "eslint --no-error-on-unmatched-pattern –-ext .ts src",
    "lint:fix": "eslint --no-error-on-unmatched-pattern –-ext .ts src --fix",
    "dev": "nodemon",
    "debug": "node --inspect=0.0.0.0:5801 --require ts-node/register ./src/server.ts",
    "start": "node dist/src/server.js",
    "prod": "npm run build && npm run start",
    "tsc": "tsc",
    "format": "prettier --no-error-on-unmatched-pattern --write \"src//*.ts\" \"test//*.ts\"",
    "pretest": "npm run build",
    "clean-husky": "rimraf .husky",
    "test": "cross-env TEST_SKIP_SDK_INIT=true mocha --reporter spec --require ts-node/register test/**/*.spec.ts --timeout 1000000 --full-trace",
    "test-local": "cross-env TEST_SKIP_SDK_INIT=true mocha --require ts-node/register test/**/*.spec.ts --timeout 1000000 -R TAP -o",
    "test-debug": "cross-env TEST_SKIP_SDK_INIT=true mocha --require ts-node/register test/**/*.spec.ts --timeout 1000000 -R TAP -o  --inspect-brk=0.0.0.0:5801",
    "test-report": "cross-env TEST_SKIP_SDK_INIT=true mocha --reporter spec --require ts-node/register test/**/*.spec.ts --timeout 1000000 --full-trace > test-report.log 2>&1",
    "prepare": "npm run clean-husky && husky install && husky add .husky/pre-push \"npm run test\""
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "keywords": [
    "iot connector"
  ],
  "license": "MIT",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "9.0.9",
    "axios": "0.24.0",
    "class-validator": "0.13.1",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "mongoose": "6.0.8",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "typedi": "0.10.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "4.2.22",
    "@types/chai-http": "4.2.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.9",
    "@types/lodash": "4.14.172",
    "@types/mocha": "9.0.0",
    "@types/moxios": "0.4.12",
    "@types/node": "14.14.9",
    "@types/sinon": "10.0.3",
    "@types/sinon-chai": "3.2.5",
    "@types/uuid": "8.3.1",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "cross-env": "7.0.3",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "mocha": "9.1.1",
    "moxios": "0.4.0",
    "nodemon": "2.0.12",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
